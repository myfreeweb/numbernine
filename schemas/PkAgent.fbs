namespace n9;

table UnixUserIdent {
	uid: string (required);
}

table UnknownIdent {
	type: string (required);
}

union Ident {
	UnixUserIdent,
	UnknownIdent,
}

table AuthRequest {
	action_id: string (required);
	cookie: string (required);
	message: string;
	icon_name: string;
	idents: [Ident];
}

root_type AuthRequest;
