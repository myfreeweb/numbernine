<?xml version="1.0" encoding="UTF-8"?>
<schemalist>
	<schema id="technology.unrelenting.numbernine.panel.panel">
		<key name="widgets" type="a(ss)">
			<default>[]</default>
			<summary>List of widgets in the panel</summary>
		</key>
	</schema>

	<schema id="technology.unrelenting.numbernine.panel.widgets.quicklaunch">
		<key name="icon-name" type="s">
			<default>'utilities-terminal'</default>
			<summary>Icon name</summary>
		</key>
		<key name="command" type="as">
			<default>[]</default>
			<summary>Command (executable path and args)</summary>
		</key>
	</schema>

	<schema id="technology.unrelenting.numbernine.panel.widgets.remoteaction">
		<key name="icon-name" type="s">
			<default>'utilities-terminal'</default>
			<summary>Icon name</summary>
		</key>
		<key name="bus-name" type="s">
			<default>'technology.unrelenting.numbernine.launcher'</default>
			<summary>D-Bus Name of the application</summary>
		</key>
		<key name="bus-path" type="s">
			<default>'/technology/unrelenting/numbernine/launcher'</default>
			<summary>D-Bus Path of the application</summary>
		</key>
		<key name="action" type="s">
			<default>'reveal'</default>
			<summary>Name of the GTK Action</summary>
		</key>
	</schema>

	<schema id="technology.unrelenting.numbernine.panel.widgets.separator">
		<key name="expand" type="b">
			<default>true</default>
			<summary>Whether to stretch the separator to maximum available space</summary>
		</key>
	</schema>

</schemalist>
