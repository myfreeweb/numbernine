[technology/unrelenting/numbernine/panel/default]
widgets=[('.remoteaction', 'launcher'), ('.quicklaunch', 'gala-launch'), ('.quicklaunch', 'term-launch'), ('.separator', 'sep-1'), ('.battery', 'bat-1'), ('.clock', 'clock')]

[technology/unrelenting/numbernine/panel/default/launcher]
icon-name='system-search'
bus-name='technology.unrelenting.numbernine.launcher'
bus-path='/technology/unrelenting/numbernine/launcher'
action='reveal'

[technology/unrelenting/numbernine/panel/default/gala-launch]
icon-name='technology.unrelenting.galacritty'
command=['/home/greg/src/github.com/myfreeweb/galacritty/target/debug/galacritty']

[technology/unrelenting/numbernine/panel/default/term-launch]
icon-name='utilities-terminal'
command=['/usr/local/bin/gnome-terminal']

[technology/unrelenting/numbernine/panel/default/sep-1]
expand=true

[technology/unrelenting/numbernine/panel/default/bat-1]
percent=true

[technology/unrelenting/numbernine/panel/default/clock]
format='%a %b %d, %R'
